{
    "name": "neondigital/laravel-locale",
    "description": "A way to handle international sites in Laravel 5",
    "keywords": [
        "Neon Digital",
        "Laravel Locale"
    ],
    "homepage": "https://github.com/neondigital/laravel-locale",
    "license": "MIT",
    "require": {
        "php" : ">=5.5.9"
    },
    "require-dev": {
        "laravel/framework": "5.2.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Neondigital\\LaravelLocale\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neondigital\\LaravelLocale\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
